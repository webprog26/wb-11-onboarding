<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <com.example.wb_11onboarding.IteoraSelectableFirstRunImageView
        android:id="@+id/primary_selectable_image_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/first_run_support_wild_animals"
        android:src="@drawable/onboarding_support_wild_life"
        android:layout_marginTop="@dimen/first_run_selectable_image_view_margin_top"
        android:layout_marginBottom="@dimen/first_run_selectable_image_view_margin_bottom"
        android:padding="@dimen/first_run_selectable_image_view_padding" />

    <com.example.wb_11onboarding.IteoraSelectableFirstRunImageView
        android:id="@+id/secondary_selectable_image_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/first_run_support_ocean_life"
        android:src="@drawable/onboarding_support_ocean_life"
        android:layout_marginTop="@dimen/first_run_selectable_image_view_margin_top"
        android:layout_marginBottom="@dimen/first_run_selectable_image_view_margin_bottom"
        android:padding="@dimen/first_run_selectable_image_view_padding" />
</merge>