<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IteoraFirstRunView">
        <attr name="android:src"/>
        <attr name="android:text"/>
        <attr name="firstRunViewTitle" format="string"/>
    </declare-styleable>

    <declare-styleable name="IteoraFirstRunViewAdvanced">
        <attr name="android:text"/>
        <attr name="firstRunViewTitleAdvanced" format="string"/>
    </declare-styleable>

    <declare-styleable name="IteoraSelectableFirstRunImageView">
        <attr name="android:src"/>
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="IteoraFirstRunSelectableView">
        <attr name="primaryImageSrc" format="integer"/>
        <attr name="secondaryImageSrc" format="integer"/>
    </declare-styleable>

    <declare-styleable name="IteoraFirstRunControllerSimple">
        <attr name="simpleActionButtonText" format="string"/>
    </declare-styleable>

    <declare-styleable name="IteoraFirstRunControllerAdvanced">
        <attr name="advancedActionButtonText" format="string"/>
        <attr name="skipButtonText" format="string"/>
    </declare-styleable>

</resources>